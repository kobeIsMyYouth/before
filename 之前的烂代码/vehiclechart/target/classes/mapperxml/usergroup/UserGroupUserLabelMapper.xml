<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.hfut.vehiclechart.mapper.usergroup.UserGroupUserLabelMapper">
	<resultMap id="BaseResultMap" type="team.hfut.vehiclechart.bean.usergroup.UserGroupUserLabelBean">
		<id column="id" property="id" />
		<result column="user_type" property="user_type" />
		<result column="user_label" property="user_label" />
		<result column="user_num" property="user_num" />
		<result column="create_time" property="create_time" />
		<result column="version" property="version" />
	</resultMap>

	<sql id="Base_Column_List">
		user_type,user_label,user_num,create_time,version
	</sql>

	<select id="selectStrList" resultType="string" parameterType="team.hfut.vehiclechart.bean.usergroup.UserGroupUserLabelBean">
		select user_label
		from result_usergroup_userlabel
		where 1=1
		<if test="user_type != 'all'.toString() ">
			and find_in_set(user_type,#{user_type})
		</if>
	</select>

	<select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="string">
		select
		<include refid="Base_Column_List" />
		from result_usergroup_userlabel
		where id = #{id}
	</select>

	<select id="selectList" resultMap="BaseResultMap"
		parameterType="team.hfut.vehiclechart.bean.usergroup.UserGroupUserLabelBean">
		select
		<include refid="Base_Column_List" />
		from result_usergroup_userlabel
		<where>
			<if test="id != null   ">
				and id = #{id}
			</if>
			<if test="user_type != null and user_type != ''  ">
				and user_type = #{user_type}
			</if>
			<if test="user_label != null and user_label != ''  ">
				and user_label = #{user_label}
			</if>
			<if test="user_num != null   ">
				and user_num = #{user_num}
			</if>
			<if test="create_time != null and create_time != ''  ">
				and create_time = #{create_time}
			</if>
			<if test="version != null   ">
				and version = #{version}
			</if>
		</where>
	</select>

	<select id="selectCount" resultType="int"
		parameterType="team.hfut.vehiclechart.bean.usergroup.UserGroupUserLabelBean">
		select count(0)
		from result_usergroup_userlabel
		<where>
			<if test="id != null  ">
				and id = #{id}
			</if>
			<if test="user_type != null and user_type != '' ">
				and user_type = #{user_type}
			</if>
			<if test="user_label != null and user_label != '' ">
				and user_label = #{user_label}
			</if>
			<if test="user_num != null  ">
				and user_num = #{user_num}
			</if>
			<if test="create_time != null and create_time != '' ">
				and create_time = #{create_time}
			</if>
			<if test="version != null  ">
				and version = #{version}
			</if>
		</where>
	</select>
</mapper>
